import turtle
t = turtle.Turtle()

t.pensize(10)

t.color('blue')
t.up()
t.goto(-200,50)
t.down()
t.circle(100)

t.color('black')
t.up()
t.goto(0,50)
t.down()
t.circle(100)

t.color('red')
t.up()
t.goto(200,50)
t.down()
t.circle(100)

t.color('yellow')
t.up()
t.goto(-100,-50)
t.down()
t.circle(100)

t.color('green')
t.up()
t.goto(100,-50)
t.down()
t.circle(100)
